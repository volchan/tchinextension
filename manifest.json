{
  "manifest_version": 2,
  "name": "Tchïn chrome extension",
  "description": "Know when Tchïn is live !",
  "version": "1.0",
  "permissions": ["http://*/*", "https://*/*", "notifications"],
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "/popup/popup.html"
  },
  "icons": {
    "128": "img/icon_72.png"
  },
  "background": {
    "scripts": ["/js/background.js"]
  },
  "author": {
      "main": "Volchan"
  }
}
